    "df = pd.read_csv('forestfires.csv', error_bad_lines = False)\n",
    "# Отбрасываем колонки со строковыми значениями\n",
    "df = df.drop('month',1) \n",
    "df = df.drop('day',1) \n",
    "df = df.interpolate()\n",
    "df.head(10) # Смотрим первые 10 строк таблицы"
   ]
